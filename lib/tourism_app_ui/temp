CarouselSlider(
        items: List.generate(
          10,
          (index) => Container(
            //color: Colors.blueGrey,
              decoration: BoxDecoration(
                //color: Colors.blueGrey,
            image: DecorationImage(
              image: AssetImage(fimage[index]),
            ),
          )),
        ),
        options: CarouselOptions(
            height: 200,
            viewportFraction: 0.6,
            initialPage: 3,
            autoPlay: true,
            enlargeCenterPage: true,
            reverse: true),
      ),